{
    "name": "ozee31/cakephp-override",
    "description": "Easily override plugin classes and templates for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7|^6.0"
    },
    "autoload": {
        "psr-4": {
            "Override\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Override\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
